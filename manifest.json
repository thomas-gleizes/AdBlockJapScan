{
  "name": "AdBlockPlusUltra",
  "author": "Kalat",
  "description": "AdBlockPlusUltra",
  "version": "2.2",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "favicons.png",
    "default_popup": "popUp.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["app.js"]
  }],
  "permissions": ["storage", "webNavigation", "activeTab"],
  "manifest_version": 2,
  "icons": {
    "128": "favicons.png"
  }
 }